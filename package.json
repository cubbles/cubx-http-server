  "description": "A command line http-server based on the popular http-server npm module able to work behind proxy.",
  "main": "./lib/cubx-http-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/cubbles/cubx-http-server"
  },
  "keywords": [
    "cli",
    "command"
  ],
  "scripts": {
