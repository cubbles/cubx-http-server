  "contributors": [
    {
      "name": "Philipp Wagner",
      "email": "philipp.wagner@incowia.com"
    },
    {
      "name": "Hans-Dieter Böhlau",
      "email": "hans-dieter.boehlau@incowia.com"
    },
    {
